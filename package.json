    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng:serve:app-c9": "ng serve app --host 0.0.0.0 --port 8080 --disableHostCheck",
    "ng:serve:app": "ng serve --app app -p 8080 -H 0.0.0.0 --public-host ${HOST}",
    "ng:serve:app-customer": "ng serve --app app-customer -p 8080 -H 0.0.0.0 --public-host ${HOST}",
    "ng:serve:app-employee": "ng serve --app app-employee -p 8080 -H 0.0.0.0 --public-host ${HOST}",
    "ng:build:app": "ng build --app app --aot true --environment prod --output-hashing all --sourcemaps true --extract-css true --named-chunks false --progress true --verbose true",
